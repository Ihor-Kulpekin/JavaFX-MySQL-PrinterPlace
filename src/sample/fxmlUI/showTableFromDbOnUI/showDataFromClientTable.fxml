<AnchorPane prefHeight="479.0" prefWidth="711.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.controllers.showDataFromDbControllers.ShowDataFromClientTableController">
   <children>
      <Label layoutX="25.0" layoutY="25.0" text="Таблиця Замовник">
         <font>
            <Font size="23.0" />
         </font>
      </Label>
      <TableView fx:id="tableView" layoutX="25.0" layoutY="69.0" prefHeight="396.0" prefWidth="652.0">
        <columns>
            <TableColumn fx:id="colId_Client" prefWidth="86.0" text="id_Замовник" />
            <TableColumn fx:id="colName_Client" prefWidth="63.0" text="Імя" />
            <TableColumn fx:id="colLast_Name" prefWidth="104.0" text="Прізвище" />
            <TableColumn fx:id="colCode_Client" prefWidth="99.0" text="Код Замовника" />
            <TableColumn fx:id="colMobile_Number" prefWidth="83.0" text="Номер" />
            <TableColumn fx:id="colEmail" minWidth="1.0" prefWidth="105.0" text="Електрога пошта" />
        </columns>
      </TableView>
      <Button fx:id="buttonShowDataFromClientTable" layoutX="234.0" layoutY="30.0" mnemonicParsing="false" onAction="#showTableClient" text="Відобразити таблицю" />
      <Button fx:id="buttonOpenFormChanging" layoutX="381.0" layoutY="30.0" mnemonicParsing="false" onAction="#clickButtonOpenFormChanging" text="форма для редагування" />
      <Button fx:id="buttonOpenFormDeleting" onAction="#clickButtonOpenFormDeleting" layoutX="539.0" layoutY="30.0" mnemonicParsing="false" text="Форма для видалення" />
   </children>
</AnchorPane>
